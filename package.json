{
	"name": "whatodo-ui",
	"productName": "whatodo-ui",
	"version": "0.0.1",
	"description": "Whatodo UI",
	"main": "./main.js",
	"scripts": {
		"prod": "webpack --env=prod --mode production",
		"clean": "npm outdated && npm prune",
		"test": "mocha --trace-warnings",
		"estart": "electron-forge start",
		"start": "npm run prod && electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"lint": "eslint src --color",
		"cleanInstall": "rm -rf package-lock.json node_modules/ && npm i",
		"dev": "webpack-dev-server --env=dev --mode development",
		"todo": "./node_modules/whatodo/cli.js -i ./ -o TODOS.json -f JSON && ./node_modules/whatodo/cli.js",
		"docs": "jsdoc ./*.js ./**/*.js -R ./README.md -d docs/"
	},
	"keywords": [],
	"author": "Parellin Technologies LLC",
	"bugs": {
		"url": "https://github.com/Parellin-Technologies-LLC/whatodo-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Parellin-Technologies-LLC/whatodo-ui.git"
	},
	"license": "MIT",
	"config": {
		"forge": {
			"make_targets": {
				"win32": [
					"squirrel"
				],
				"darwin": [
					"zip"
				],
				"linux": [
					"deb",
					"rpm"
				]
			},
			"electronPackagerConfig": {
				"packageManager": "npm"
			},
			"electronWinstallerConfig": {
				"name": "whatodo_ui"
			},
			"electronInstallerDebian": {},
			"electronInstallerRedhat": {},
			"github_repository": {
				"owner": "",
				"name": ""
			},
			"windowsStoreConfig": {
				"packageName": "",
				"name": "whatodoui"
			}
		}
	},
	"dependencies": {
		"bootstrap": "^4.0.0",
		"electron-compile": "^6.4.2",
		"electron-squirrel-startup": "^1.0.0",
		"font-awesome": "^4.7.0",
		"jquery": "^3.3.1",
		"popper.js": "^1.14.1",
		"whatodo": "^0.4.15"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.4",
		"babel-plugin-transform-async-to-generator": "^6.24.1",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"babelify": "^8.0.0",
		"chai": "^4.1.2",
		"chai-as-promised": "^7.1.1",
		"copy-webpack-plugin": "^4.5.1",
		"css-loader": "^0.28.10",
		"electron": "^1.8.3",
		"electron-forge": "^5.1.1",
		"electron-prebuilt-compile": "1.8.2",
		"electron-webpack": "^2.0.1",
		"eslint": "^3.19.0",
		"eslint-config-airbnb": "^15.1.0",
		"eslint-plugin-import": "^2.9.0",
		"eslint-plugin-jsx-a11y": "^5.1.1",
		"eslint-plugin-react": "^7.7.0",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "^1.1.11",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^3.0.6",
		"json-loader": "^0.5.7",
		"mocha": "^5.0.4",
		"node-sass": "^4.7.2",
		"optimize-css-assets-webpack-plugin": "^4.0.0",
		"postcss-loader": "^2.1.1",
		"precss": "^3.1.2",
		"raw-loader": "^0.5.1",
		"sass-loader": "^6.0.7",
		"style-loader": "^0.20.3",
		"uglifyjs-webpack-plugin": "^1.2.3",
		"url-loader": "^1.0.1",
		"webpack": "^4.1.1",
		"webpack-cli": "^2.0.13",
		"webpack-dev-server": "^3.1.1",
		"webpack-merge": "^4.1.2"
	}
}
