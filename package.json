{
	"name": "whatodo-ui",
	"productName": "whatodo-ui",
	"version": "0.1.0",
	"description": "Whatodo UI",
	"main": "./src/main.js",
	"scripts": {
		"clean": "npm outdated && npm prune",
		"test": "mocha --trace-warnings",
		"estart": "electron-forge start",
		"scss": "./node_modules/sass/sass.js  ./src/scss/main.scss ./src/style.css",
		"start": "npm run scss && electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"lint": "eslint src --color",
		"cleanInstall": "rm -rf package-lock.json node_modules/ && npm i",
		"todo": "./node_modules/whatodo/cli.js -i ./ -o TODOS.json -f JSON && ./node_modules/whatodo/cli.js",
		"docs": "jsdoc ./*.js ./**/*.js -R ./README.md -d docs/"
	},
	"keywords": [],
	"author": "Parellin Technologies LLC",
	"bugs": {
		"url": "https://github.com/Parellin-Technologies-LLC/whatodo-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Parellin-Technologies-LLC/whatodo-ui.git"
	},
	"license": "MIT",
	"config": {
		"forge": {
			"make_targets": {
				"win32": [
					"squirrel"
				],
				"darwin": [
					"zip",
					"dmg"
				],
				"linux": [
					"deb",
					"rpm"
				]
			},
			"electronPackagerConfig": {
				"packageManager": "npm"
			},
			"electronWinstallerConfig": {
				"name": "whatodo_ui"
			},
			"electronInstallerDebian": {},
			"electronInstallerRedhat": {},
			"github_repository": {
				"owner": "",
				"name": ""
			},
			"windowsStoreConfig": {
				"packageName": "",
				"name": "whatodoui"
			}
		}
	},
	"dependencies": {
		"bootstrap": "^4.0.0",
		"electron": "^1.8.4",
		"electron-compile": "^6.4.2",
		"electron-squirrel-startup": "^1.0.0",
		"font-awesome": "^4.7.0",
		"jquery": "^3.3.1",
		"popper.js": "^1.14.1",
		"whatodo": "^0.4.15"
	},
	"devDependencies": {
		"babel-plugin-transform-async-to-generator": "^6.24.1",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"chai": "^4.1.2",
		"chai-as-promised": "^7.1.1",
		"electron-forge": "^5.1.1",
		"electron-prebuilt-compile": "1.8.2",
		"eslint": "^3.19.0",
		"eslint-config-airbnb": "^15.1.0",
		"eslint-plugin-import": "^2.9.0",
		"eslint-plugin-jsx-a11y": "^5.1.1",
		"eslint-plugin-react": "^7.7.0",
		"mocha": "^5.0.4",
		"sass": "^1.1.1"
	}
}
